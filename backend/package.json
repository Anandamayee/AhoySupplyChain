{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "Anandamayee Nanda",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/tea-process/main",
    "lint": "eslint \"{src,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./backend/test/jest-e2e.json",
    "start:tea": "npx tsc && node dist/tea-process/main.js",
    "start:auth": "npx tsc && node dist/user-auth/main.js",
    "build:user-auth": "nest build user-auth",
    "build:tea-process": "nest build tea-process",
    "start:tea-process": "nest start tea-process --watch",
    "start:user-auth": "nest start user-auth --watch"
  },
  "dependencies": {
    "@ethersproject/abi": "~5.6.1",
    "@ethersproject/providers": "~5.6.4",
    "@nestjs/axios": "~3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "~10.2.0",
    "@nestjs/mongoose": "~10.0.6",
    "@nestjs/passport": "~10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.0",
    "@types/bcrypt": "~5.0.2",
    "@types/uuid": "~9.0.8",
    "axios": "~1.7.2",
    "bcrypt": "~5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "crypto-js": "~4.2.0",
    "ethers": "^5.7.2",
    "express-rate-limit": "^7.4.0",
    "express-session": "^1.18.0",
    "fs": "^0.0.1-security",
    "helmet": "^7.1.0",
    "jsonwebtoken": "~9.0.2",
    "kafkajs": "^2.2.4",
    "mongoose": "^8.4.1",
    "ms": "~2.1.3",
    "nest-winston": "^1.9.7",
    "passport": "~0.7.0",
    "passport-jwt": "~4.0.1",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "~10.0.0",
    "web3": "^4.12.1",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^6.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.16.2",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/",
      "<rootDir>/shared/"
    ],
    "moduleNameMapper": {
      "^@app/db-utilites(|/.*)$": "<rootDir>/shared/db-utilites/src/$1",
      "^@app/eth-utilites(|/.*)$": "<rootDir>/shared/eth-utilites/src/$1",
      "^@app/queue-utilites(|/.*)$": "<rootDir>/shared/queue-utilites/src/$1",
      "^@app/user-guard(|/.*)$": "<rootDir>/shared/user-guard/src/$1",
      "^@app/exceptions(|/.*)$": "<rootDir>/shared/exceptions/src/$1"
    }
  }
}
